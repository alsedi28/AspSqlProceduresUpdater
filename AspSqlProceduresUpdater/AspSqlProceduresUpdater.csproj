<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Alsedi28</RootNamespace>
    <Authors>Sergey Ignatushchenko</Authors>
    <RepositoryUrl>https://github.com/alsedi28/AspSqlProceduresUpdater</RepositoryUrl>
    <RepositoryType></RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>This package allows you to update the custom stored procedures in the database that are in the project when you run the application.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.1" />
  </ItemGroup>

</Project>
